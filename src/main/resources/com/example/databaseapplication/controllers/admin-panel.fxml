            <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="186.0" layoutY="34.0" prefHeight="32.0" prefWidth="207.0" text="Admin Panel" />
            <Button fx:id="logoutButton" layoutX="505.0" layoutY="21.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" prefHeight="18.0" prefWidth="81.0" text="Logout" />
            <Label layoutX="186.0" layoutY="97.0" prefHeight="18.0" prefWidth="105.0" text="Game World list" />
